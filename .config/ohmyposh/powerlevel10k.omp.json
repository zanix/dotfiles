{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "black",
          "background": "#d3d7cf",
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": "",
          "template": "{{ if .WSL }}WSL at {{ end }}{{.Icon}}",
          "type": "os"
        },
        {
          "background": "transparent",
          "foreground": "#42a9ff",
          "properties": {
            "home_icon": "~",
            "style": "full"
          },
          "style": "plain",
          "template": " \uf07c {{ .Path }}",
          "type": "path"
        },
        {
          "background": "transparent",
          "foreground": "#6c6c6c",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#ffeb3b{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#f26d50{{ end }}",
            "{{ if gt .Ahead 0 }}#89d1dc{{ end }}",
            "{{ if gt .Behind 0 }}#4e9a06{{ end }}"
          ],
          "properties": {
            "branch_icon": "\uf126 ",
            "commit_icon": "\uf1fa",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "plain",
          "template": " {{ .UpstreamIcon }} {{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "type": "git"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "background": "transparent",
          "foreground": "#689f63",
          "properties": {
            "fetch_version": true
          },
          "style": "plain",
          "template": " {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} \ue718 ",
          "type": "node"
        },
        {
          "background": "transparent",
          "foreground": "#00acd7",
          "properties": {
            "fetch_version": true
          },
          "style": "plain",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} \ue627 ",
          "type": "go"
        },
        {
          "background": "transparent",
          "foreground": "#4063D8",
          "properties": {
            "fetch_version": true
          },
          "style": "plain",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} \ue624 ",
          "type": "julia"
        },
        {
          "background": "transparent",
          "foreground": "#4063D8",
          "properties": {
            "fetch_version": true
          },
          "style": "plain",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} \ued6d ",
          "type": "php"
        },
        {
          "background": "transparent",
          "foreground": "#FFDE57",
          "properties": {
            "display_mode": "files",
            "fetch_virtual_env": false
          },
          "style": "plain",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} \ue235 ",
          "type": "python"
        },
        {
          "background": "transparent",
          "foreground": "#AE1401",
          "properties": {
            "display_mode": "files",
            "fetch_version": true
          },
          "style": "plain",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} \ue791 ",
          "type": "ruby"
        },
        {
          "background": "transparent",
          "foreground": "#FEAC19",
          "properties": {
            "display_mode": "files",
            "fetch_version": false
          },
          "style": "plain",
          "template": " {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} \uf0e7",
          "type": "azfunc"
        },
        {
          "background": "transparent",
          "foreground_templates": [
            "{{if contains \"default\" .Profile}}#FFA400{{end}}",
            "{{if contains \"jan\" .Profile}}#f1184c{{end}}"
          ],
          "foreground": "#ffffff",
          "properties": {
            "display_default": false
          },
          "style": "plain",
          "template": " {{ .Profile }}{{ if .Region }}@{{ .Region }}{{ end }} \ue7ad ",
          "type": "aws"
        },
        {
          "background": "transparent",
          "foreground": "#ffff66",
          "style": "plain",
          "template": " \uf0ad ",
          "type": "root"
        },
        {
          "background": "transparent",
          "foreground": "#ffeb3b",
          "properties": {
            "threshold": 5000
          },
          "style": "plain",
          "template": " {{ .FormattedMs }} \uf252 ",
          "type": "executiontime"
        },
        {
          "background": "transparent",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#cc2222{{ end }}",
            "{{ if eq .Code 0 }}#d3d7cf{{ end }}"
          ],
          "foreground": "#d3d7cf",
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": " {{ if gt .Code 0 }}{{ reason .Code }}{{ else }}\uf42e{{ end }} ",
          "type": "status"
        },
        {
          "background": "transparent",
          "foreground": "#444444",
          "style": "plain",
          "template": "\u2500\u256e",
          "type": "text"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "background": "transparent",
          "foreground": "#444444",
          "style": "plain",
          "template": "\u2570\u2500",
          "type": "text"
        }
      ],
      "type": "prompt"
    },
    {
      "segments": [
        {
          "background": "transparent",
          "foreground": "#444444",
          "style": "plain",
          "template": "\u2500\u256f",
          "type": "text"
        }
      ],
      "type": "rprompt"
    }
  ],
  "console_title_template": "{{ .UserName }}@{{ .HostName }} {{ .Shell }} in {{ .PWD }}",
  "transient_prompt": {
    "background": "transparent",
    "foreground": "green",
    "foreground_templates": [
      "{{ if gt .Code 0 }}#cc2222{{ end }}",
      "{{ if eq .Code 0 }}green{{ end }}"
    ],
    "template": "\u276f "
  },
  "secondary_prompt": {
    "background": "transparent",
    "foreground": "#d3d7cf",
    "template": "\u276f\u276f "
  },
  "final_space": true,
  "version": 3
}
